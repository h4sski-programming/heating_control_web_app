@startuml

object App {
+ rooms_list: list[Room]
}

object Room {
+ name: str
+ temperature: float
+ temp_plan: list[TemperaturePlan]
}

object TemperaturePlan {
+ segments_list: list(Segment)
}

object Segment {
- time_start: time
- time_end: time
- temp_min: float
- temp_max: float
}

App "1" o-- "0..n" Room
Room <-- TemperaturePlan
TemperaturePlan o-- Segment

@enduml